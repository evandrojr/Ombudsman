        </id>

        <property name="logradouro" type="string" column="dsc_lograd" access="field" />

        <property name="numero" type="string" column="num_lograd" access="field" />

        <property name="complemento" type="string" column="dsc_compl_lograd" access="field" />
        <property name="bairro" type="string" column="dsc_bairro" access="field" />
        <property name="cidade" type="string" column="dsc_cidade" access="field" />
        <property name="cep" type="string" column="num_cep" access="field" />

        <many-to-one name="uf" class="br.gov.serpro.ouvidoria.model.UF" column="cod_uf" cascade="none" />

        <many-to-one name="pais" class="br.gov.serpro.ouvidoria.model.Pais" column="cod_pais" cascade="none" />

    </class>

    <class name="br.gov.serpro.ouvidoria.model.Acionador" table="acionador" discriminator-value="A">

        <id name="id" type="long" column="cod_acndor">
            <generator class="native" />
        </id>

        <discriminator column="tip_cpf_cnpj" type="string" />

        <property name="sexo" type="string" column="tip_sexo" access="field" />
        <property name="email" type="string" column="dsc_email" access="field" />
        <property name="codigoPaisTelefone" type="string" column="dsc_ddi_tel" access="field" />
        <property name="codigoCidadeTelefone" type="string" column="dsc_ddd_tel" access="field" />
        <property name="telefone" type="string" column="dsc_tel" access="field" />
        <property name="codigoPaisFax" type="string" column="dsc_ddi_fax" access="field" />
        <property name="codigoCidadeFax" type="string" column="dsc_ddd_fax" access="field" />
        <property name="fax" type="string" column="dsc_fax" access="field" />

        <many-to-one name="escolaridade" column="cod_escola" class="br.gov.serpro.ouvidoria.model.Escolaridade" cascade="none" />
        <many-to-one name="faixaEtaria" column="cod_faixa_etaria" class="br.gov.serpro.ouvidoria.model.FaixaEtaria" cascade="none" />
        <many-to-one name="meioEnvioRespostaOrgao" column="cod_meio_envio_resp_orgao" class="br.gov.serpro.ouvidoria.model.MeioEnvioRespostaOrgao" cascade="none" />
        <many-to-one name="tipoAcionador" column="cod_tipo_acndor" class="br.gov.serpro.ouvidoria.model.TipoAcionador" cascade="none" />
        <many-to-one name="uf" column="cod_uf" class="br.gov.serpro.ouvidoria.model.UF" cascade="none" />
        <many-to-one name="pais" column="cod_pais" class="br.gov.serpro.ouvidoria.model.Pais" cascade="none" />
        <many-to-one name="informacoesRecebimentoRespostaCarta" column="cod_meio_receb_resp" class="br.gov.serpro.ouvidoria.model.InformacoesRecebimentoRespostaCarta" cascade="all" />
        <many-to-one name="informacoesConsultaAcionamento" column="cod_cons_acnmnt" class="br.gov.serpro.ouvidoria.model.InformacoesConsultaAcionamento" cascade="all" />

        <subclass name="br.gov.serpro.ouvidoria.model.AcionadorPessoaFisica" discriminator-value="F" proxy="br.gov.serpro.ouvidoria.model.AcionadorPessoaFisica">

