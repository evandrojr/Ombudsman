        <property name="imagemLogo" type="string" column="nom_img_logo" />
        <property name="imagemBanner" type="string" column="nom_img_banner" />
        <property name="imagem3" type="string" column="nom_img_3" />
        <property name="imagem4" type="string" column="nom_img_4" />
        <property name="imagem5" type="string" column="nom_img_5" />
        <property name="descricaoImagemLogo" type="string" column="dsc_img_logo" />
        <property name="descricaoImagemBanner" type="string" column="dsc_img_banner" />
        <property name="descricaoImagem3" type="string" column="dsc_img_3" />
        <property name="descricaoImagem4" type="string" column="dsc_img_4" />
        <property name="descricaoImagem5" type="string" column="dsc_img_5" />
        <property name="linkImagemLogo" type="string" column="url_img_logo" />
        <property name="linkImagemBanner" type="string" column="url_img_banner" />
        <property name="linkImagem3" type="string" column="url_img_3" />
        <property name="linkImagem4" type="string" column="url_img_4" />
        <property name="linkImagem5" type="string" column="url_img_5" />
        <property name="urlBaseFuncionarios" type="string" column="dsc_url_base_func" />
        <property name="textoConsultaRespostaNoPrazo" type="string" column="dsc_texto_cons_resp_no_prazo" />
        <property name="textoConsultaRespostaSemPrazo" type="string" column="dsc_texto_cons_resp_sem_prazo" />
        <property name="textoConsultaRespostaEmAtraso" type="string" column="dsc_texto_cons_resp_em_atraso" />
