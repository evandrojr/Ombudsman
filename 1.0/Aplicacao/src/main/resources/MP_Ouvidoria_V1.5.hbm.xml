        <property name="horaEnvioNotificacao" type="string" column="HOR_ENVIO_NOTIFICACAO" />
    </class>

    <class name="br.gov.serpro.ouvidoria.model.Instituicao" table="instituicao">

        <id name="id" type="long" column="cod_instit">
            <generator class="native" />
        </id>

        <discriminator column="ind_tipo_instit" type="string" />

        <property name="descricao" type="string" column="dsc_instit" />
        <property name="dataInicioOperacao" type="timestamp" column="dat_ini_oper" />
        <property name="dataFimOperacao" type="timestamp" column="dat_fim_oper" />
        <property name="dataInicioCadastramento" type="timestamp" column="dat_ini_cadast" />
        <property name="dataFimCadastramento" type="timestamp" column="dat_fim_cadast" />
        <property name="dataCadastramento" type="timestamp" column="dat_cadast" />

        <set name="listaAssunto" table="assunto">
            <key column="cod_instit" />
            <one-to-many class="br.gov.serpro.ouvidoria.model.Assunto" />
        </set>

        <subclass name="br.gov.serpro.ouvidoria.model.Orgao" discriminator-value="0">

            <property name="dataInicioAcionamento" type="timestamp" column="dat_ini_acnmnt" />
            <property name="dataFimAcionamento" type="timestamp" column="dat_fim_acnmnt" />
            <property name="dataInicioConsultaResposta" type="timestamp" column="dat_ini_cons_resp" />
            <property name="dataFimConsultaResposta" type="timestamp" column="dat_fim_cons_resp" />
            <property name="gestorResponsavel" type="string" column="nom_respon" />
            <property name="telefoneContatoGestor" type="string" column="num_tel_respon" />
            <property name="codigoPaisTelefoneResponsavel" type="string" column="dsc_ddi_respon" />
            <property name="codigoCidadeTelefoneResponsavel" type="string" column="dsc_ddd_respon" />
            <property name="statusOrgao" type="string" column="ind_situa_orgao" />

            <many-to-one name="configuracoes" column="cod_config" class="br.gov.serpro.ouvidoria.model.ConfiguracoesOrgao" cascade="all" />

            <set name="listaGruposEmail" table="gruposemail">
                <key column="cod_instit" />
                <one-to-many class="br.gov.serpro.ouvidoria.model.GruposEmail" />
            </set>

            <set name="listaMeioEnvioRespostaOrgao" table="meioenviorespostaorgao" where="( DAT_VIGENC IS NULL OR DAT_VIGENC >= CURRENT_TIMESTAMP )">
                <key column="cod_instit" />
                <one-to-many class="br.gov.serpro.ouvidoria.model.MeioEnvioRespostaOrgao" />
            </set>

            <set name="listaSubOrgao" table="instituicao" where="( DAT_FIM_VIGENC IS NULL OR DAT_FIM_VIGENC >= CURRENT_TIMESTAMP )">
                <key column="cod_instit_pai" />
                <one-to-many class="br.gov.serpro.ouvidoria.model.SubOrgao" />
            </set>

            <set name="listaTodosSubOrgaos" table="instituicao" >
                <key column="cod_instit_pai" />
                <one-to-many class="br.gov.serpro.ouvidoria.model.SubOrgao" />
            </set>

            <set name="listaTipoAvaliacaoResposta" table="tipoavaliacaoresposta" where="( DAT_VIGENC IS NULL OR DAT_VIGENC >= CURRENT_TIMESTAMP )">
                <key column="cod_instit" />
                <one-to-many class="br.gov.serpro.ouvidoria.model.TipoAvaliacaoResposta" />
            </set>

            <set name="listaTipoAcionadorOrgao" table="tipoacionadororgao" where="( DAT_VIGENC IS NULL OR DAT_VIGENC >= CURRENT_TIMESTAMP )">
                <key column="cod_instit" />
                <one-to-many class="br.gov.serpro.ouvidoria.model.TipoAcionadorOrgao" />
            </set>

            <set name="listaTipoMensagem" table="tipomensagem" where="( DAT_VIGENC IS NULL OR DAT_VIGENC >= CURRENT_TIMESTAMP )">
                <key column="cod_instit" />
                <one-to-many class="br.gov.serpro.ouvidoria.model.TipoMensagem" />
            </set>

        </subclass>

        <subclass name="br.gov.serpro.ouvidoria.model.SubOrgao" discriminator-value="1">

            <property name="dataInicioVigencia" type="timestamp" column="dat_ini_vigenc" />
            <property name="dataFimVigencia" type="timestamp" column="dat_fim_vigenc" />

            <many-to-one name="orgao" column="cod_instit_pai" class="br.gov.serpro.ouvidoria.model.Orgao" />

            <set name="listaLocalidadeOcorrencia" table="localidadeocorrencia" where="( DAT_VIGENC IS NULL OR DAT_VIGENC >= CURRENT_TIMESTAMP )">
                <key column="cod_instit" />
                <one-to-many class="br.gov.serpro.ouvidoria.model.LocalidadeOcorrencia" />
            </set>


        </subclass>

    </class>

    <class name="br.gov.serpro.ouvidoria.model.Assunto" table="assunto">

        <id name="id" type="long" column="cod_assnto">
            <generator class="native" />
        </id>

        <property name="descricao" type="string" column="dsc_assnto" />
        <property name="descricaoDetalhada" type="string" column="dsc_detalh_assnto" />
        <property name="prazoInterno" type="integer" column="qtd_prazo_int" />
        <property name="prazoExterno" type="integer" column="qtd_prazo_ext" />
        <property name="dataCadastramento" type="timestamp" column="dat_cadast" />
        <property name="dataVigencia" type="timestamp" column="dat_vigenc" />

        <many-to-one name="instituicao" column="cod_instit" class="br.gov.serpro.ouvidoria.model.Instituicao" />

        <set name="listaFuncionario" table="funcionarioassunto">
            <key column="cod_assnto" />
            <many-to-many class="br.gov.serpro.ouvidoria.model.Funcionario" column="cod_func" />
        </set>


    </class>

    <class name="br.gov.serpro.ouvidoria.model.TipoMensagem" table="tipomensagem">

        <id name="id" type="long" column="cod_tipo_msg">
            <generator class="native" />
        </id>

        <property name="descricao" type="string" column="dsc_tipo_msg" />
        <property name="descricaoDetalhada" type="string" column="dsc_detalh_tipo_msg" />
        <property name="prazoInterno" type="integer" column="qtd_prazo_int" />
        <property name="prazoExterno" type="integer" column="qtd_prazo_ext" />
        <property name="dataCadastramento" type="timestamp" column="dat_cadast" />
        <property name="dataVigencia" type="timestamp" column="dat_vigenc" />

        <many-to-one name="orgao" column="cod_instit" class="br.gov.serpro.ouvidoria.model.Orgao" />

        <set name="listaFuncionario" table="funcionariotipomensagem">
            <key column="cod_tipo_msg" />
            <many-to-many class="br.gov.serpro.ouvidoria.model.Funcionario" column="cod_func" />
        </set>

    </class>

    <class name="br.gov.serpro.ouvidoria.model.Mensagem" table="mensagem">

        <id name="id" type="long" column="cod_msg">
            <generator class="native" />
        </id>

        <property name="texto" type="string" column="dsc_texto" access="field" />
        <property name="arquivoAnexo" type="string" column="nom_arq_anexo" access="field" />
        <property name="mensagemDigitalizada" type="string" column="nom_arq_msg_digit" access="field" />

        <many-to-one name="assunto" column="cod_assnto" class="br.gov.serpro.ouvidoria.model.Assunto" cascade="none" />
        <many-to-one name="tipoMensagem" column="cod_tipo_msg" class="br.gov.serpro.ouvidoria.model.TipoMensagem" cascade="none" />

    </class>

    <class name="br.gov.serpro.ouvidoria.model.Escolaridade" table="escolaridade" mutable="false">

        <id name="id" type="long" column="cod_escola">
            <generator class="native" />
        </id>

        <property name="descricao" type="string" column="dsc_escola" />
    </class>

    <class name="br.gov.serpro.ouvidoria.model.FaixaEtaria" table="faixaetaria" mutable="false">

        <id name="id" type="long" column="cod_faixa_etaria">
            <generator class="native" />
        </id>

        <property name="intervalo" type="string" column="dsc_interv" />
        <property name="idadeInicial" type="integer" column="num_idade_ini" />
        <property name="idadeFinal" type="integer" column="num_idade_fim" />
    </class>

    <class name="br.gov.serpro.ouvidoria.model.MeioEnvioResposta" table="meioenvioresposta" mutable="false">

        <id name="id" type="long" column="cod_meio_env_resp">
