    </class>


    <class name="br.gov.serpro.ouvidoria.model.MeioEnvioRespostaOrgao" table="meioenviorespostaorgao">

        <id name="id" type="long" column="cod_meio_envio_resp_orgao">
            <generator class="native" />
        </id>

        <property name="dataVigencia" type="timestamp" column="dat_vigenc" />

        <many-to-one name="meioEnvioResposta" column="cod_meio_env_resp" class="br.gov.serpro.ouvidoria.model.MeioEnvioResposta" cascade="none" />

        <many-to-one name="orgao" column="cod_instit" class="br.gov.serpro.ouvidoria.model.Orgao" cascade="none" />

    </class>

    <class name="br.gov.serpro.ouvidoria.model.TipoAcionador" table="tipoacionador" mutable="false">

        <id name="id" type="long" column="cod_tipo_acndor">
            <generator class="native" />
        </id>

        <property name="descricao" type="string" column="dsc_tipo_acndor" />
    </class>

    <class name="br.gov.serpro.ouvidoria.model.TipoAcionadorOrgao" table="tipoacionadororgao">

        <id name="id" type="long" column="cod_tipo_acndor_orgao">
            <generator class="native" />
        </id>

        <property name="dataVigencia" type="timestamp" column="dat_vigenc" />

        <many-to-one name="tipoAcionador" column="cod_tipo_acndor" class="br.gov.serpro.ouvidoria.model.TipoAcionador" cascade="none" />

        <many-to-one name="orgao" column="cod_instit" class="br.gov.serpro.ouvidoria.model.Orgao" cascade="none" />
    </class>

    <class name="br.gov.serpro.ouvidoria.model.UF" table="uf" mutable="false">

        <id name="id" type="long" column="cod_uf">
            <generator class="native" />
        </id>

        <property name="sigla" type="string" column="dsc_sigla_uf" />
        <property name="nome" type="string" column="dsc_uf" />
    </class>

    <class name="br.gov.serpro.ouvidoria.model.InformacoesConsultaAcionamento" table="informacoesconsultaacionamento">

        <id name="id" type="long" column="cod_cons_acnmnt">
            <generator class="native" />
        </id>

        <property name="codigoAcesso" type="string" column="cod_acesso" access="field" />
        <property name="pergunta" type="string" column="dsc_perg" />
        <property name="resposta" type="string" column="dsc_resp" />

        <property name="numeroTentativasCodigo" type="integer" column="num_tent_cod" />
        <property name="numeroTentativasPergunta" type="integer" column="num_tent_perg" />

    </class>

    <class name="br.gov.serpro.ouvidoria.model.Pais" table="pais" mutable="false">

        <id name="id" type="long" column="cod_pais">
            <generator class="native" />
        </id>

        <property name="descricao" type="string" column="dsc_pais" />
    </class>

    <class name="br.gov.serpro.ouvidoria.model.InformacoesRecebimentoRespostaCarta" table="informacoesrecebimentoresposta">

        <id name="id" type="long" column="cod_meio_receb_resp">
