        <property name="qtdeTentativasPossiveisPergunta" type="integer" column="qtd_tent_protoc" />
        <property name="textoApresentacaoOuvidoria" type="string" column="dsc_texto_apres_ouvid" />
        <property name="textoApresentacaoFormulario" type="string" column="dsc_texto_apres_form" />
        <property name="textoApresentacaoResposta" type="string" column="dsc_texto_apres_resp" />
        <property name="textoRespostaAcionamento" type="string" column="dsc_texto_resp_acnmnt" />
        <property name="textoCanaisComunicacao" type="string" column="dsc_texto_canais_comuni" />
        <property name="textoInformacaoOuvidoria" type="string" column="dsc_texto_info_ouvid" />
        <property name="textoNotificacaoPendencia" type="string" column="dsc_texto_notificacao_pendencia" />
        <property name="textoNotificacaoAtraso" type="string" column="dsc_texto_notificacao_atraso" />
        <property name="textoNotificacaoAtrasoCritico" type="string" column="dsc_texto_notificacao_atraso_critico" />
