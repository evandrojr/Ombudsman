        <property name="tamanhoMaxArquivoCartaDigitalizada" type="integer" column="num_tam_max_arq_msg_digit" />
        <property name="possuiCodigoAcesso" type="boolean" column="ind_possui_codigo_acesso" />
        <property name="atendenteConsultaMsgNoAtendimento" type="boolean" column="ind_perm_atend_cons_msg" />
